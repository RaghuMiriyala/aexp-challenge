{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug: AEXP Challenge",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/services/index.js",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "skipFiles": [
                "<node_internals>/**",
                "${workspaceFolder}/node_modules/**"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "restart": true,
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--inspect"
            ]
        }
    ]
}